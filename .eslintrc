{
  "env": {
    "browser": true,
    "jasmine": true,
    "mocha": true
  },
  "globals": {
    "inject": true,
    "sinon": true,
    "ApplicationConfiguration": true
  },
  "extends": [
    "angular",
    "airbnb/base"
  ],
  "rules": {
    "camelcase": [2, {
      "properties": "always"
    }],
    "indent": [2, 4],
    "comma-dangle": [2, "never"],
    "strict": 0,
    "one-var": 0,
    "quote-props": 0,
    "space-before-function-paren": [2, {
      "anonymous": "always",
      "named": "never"
    }],
    "func-names": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "id-length": [2, {
      "exceptions": ["i", "j", "x", "y", "$", "_"]
    }],
    "wrap-iife": [2, "outside"],
    "no-param-reassign": 0,
    "no-console": 0,
    "angular/log": 0,
    "object-shorthand": 0
  }
}
